    "def superDigit(n, k):\n",
    "    \n",
    "    # Write your code here\n",
    "    \n",
    "    def soma(valor):\n",
    "        if valor < 10:\n",
    "            return valor\n",
    "        else:\n",
    "            x = sum(int(i) for i in str(valor))\n",
    "            return soma(x)\n",
    "        \n",
    "    y = soma(int(n))\n",
    "    return soma(y*k)"
   ]
